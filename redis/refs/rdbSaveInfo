<mbr r='rdbSaveInfo::repl_id_is_set' o='32' t='int'/>
<mbr r='rdbSaveInfo::repl_id' o='64' t='char [41]'/>
<mbr r='rdbSaveInfo::repl_offset' o='448' t='long long'/>
